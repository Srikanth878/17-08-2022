#include <stdio.h>
int main()
{
    int i,m,n;
    scanf("%d",&n);
   int a[n-1];
   for(i=0;i<n-1;i++)
{
   scanf("%d",&a[i]);
}
   int total=0,sum=0;
   total = (n*(n+1))/2;
    for(i=0 ; i<n-1; i++)
    {
     sum = sum + a[i];
    }
    m=total-sum;
    printf("Missing number is %d",m);
    return 0;
}
